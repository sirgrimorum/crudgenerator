{
    "name": "sirgrimorum/CrudGenerator",
    "type": "library",
    "description": "Automatic CRUD generator for Laravel",
    "keywords": [
        "Sirgrimorum",
        "CrudGenerator"
    ],
    "homepage": "https://desarrollo.grimorum.com/andres/CrudGenerator",
    "license": "MIT",
    "authors": [
        {
            "name": "Sirgrimorum",
            "email": "andres.espinosa@grimorum.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "laravel/framework": ">=5.6",
        "laravelcollective/html": "^5.6",
        "intervention/image": "^2.4",
        "doctrine/dbal": "^2.6"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3"
    },
    "autoload": {
        "psr-4": {
            "Sirgrimorum\\CrudGenerator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.0-dev"
        },
        "laravel": {
            "providers": [
                "Sirgrimorum\\GrudGenerator\\CrudGeneratorServiceProvider"
            ],
            "aliases": {
                "CrudLoader": "Sirgrimorum\\GrudGenerator\\GrudGeneratorFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    }
}
